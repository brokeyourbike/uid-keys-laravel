{
    "name": "brokeyourbike/uid-keys",
    "description": "A simple drop-in solution for providing UUID and ULID support for the IDs of your Eloquent models.",
    "type": "library",
    "license": "MPL-2.0",
    "keywords": [
        "php",
        "laravel",
        "eloquent",
        "uuid",
        "ulid"
    ],
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "ivan@stasi.uk",
            "homepage": "https://stasi.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "BrokeYourBike\\UidKeys\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrokeYourBike\\UidKeys\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "symfony/uid": "^7.4"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8.3",
        "orchestra/testbench": "*",
        "phpunit/phpunit": "^10"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "version": "0.1.2"
}
